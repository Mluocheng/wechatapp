.container, .header {
  background-color: #2196f3;
}

.container {
  position: fixed;
  top: 0;
  width: 100%;
}

.header {
  width: 100%;
  height: 80rpx;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  align-items: center;
}

.header button {
  font-size: 16px;
  height: 80rpx;
  display: inline-block;
  line-height: 80rpx;
  background-color: transparent;
  border-color: #2196f3;
  color: #fff;
}
.hoverbtnsty{
  color: rgb(182, 182, 182)
}
.lf {
  float: left;
}

.rf {
  float: right;
}

.clear {
  clear: both;
}

.box {
  width: 95%;
  margin: 20rpx auto;
  font-size: 28rpx;
}

.flex_box {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  font-size: 28rpx;
}

.flex_box .flexlist {
  align-self: flex-start;
  color: #000;
  border-radius: 10rpx;
  margin: 10rpx auto;
}

.flex_box .title {
  color: #000;
  font-size: 28rpx;
  font-weight: 700;
}

.flex_box .flexlisttitle {
  width: 100%;
  text-align: center;
  padding: 10rpx auto;
  font-weight: 700;
}

.flex_box .flexlisttime {
  background-color: transparent;
  color: rgb(99, 99, 99);
  font-size: 28rpx;
  width:100%;
}

.flex_box .flexlisttime>view {
  margin: 20rpx auto;
}
.procontenttitle{
  text-align: center
}


.flex_box .flexlisttime text {
  /* margin-left: 20rpx; */
  font-size: 28rpx;
}

.flex_box .flexlistcon {
  font-size: 28rpx;
}

.flex_box .flexlistcon>view {
  margin: 20rpx;
}

.flex_box .procontentkeyword, .flex_box .procontentintroduction {
  text-align: left;
}

.flex_box .procontent {
  color: rgb(99, 99, 99);
}

.flex_box .colorblur {
  color: #38f;
}

.flex_box .colororg {
  color: #ff9800;
  text-decoration: underline;
  word-break: break-all;
}
.projectoritext,.intordu,.time{
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
}
.ti{
  word-break:break-all;
  white-space:nowrap; 
}
.urlti{
  margin-left:30rpx;
  overflow:hidden; 
    text-overflow:ellipsis;
    white-space:nowrap; 
}

.bcc {
  width: 100%;
  background-color: #f7feff;
  display: block;
  height: 20rpx;
}
.tenderdocuments{
  text-align: center;
  width:100%;
margin:10px auto;

}
.tenderdocuments .procontent{
display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 26rpx;
}
.tenderdocuments .procontent .prosee{
color: #6eb3ec;
border: 1px solid #6eb3ec;
border-radius: 10px;
 width: 50px;
}
.hoversty{
  background-color: #6eb3ec;
  color: #fff
}